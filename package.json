{
    "name": "tellmejoke",
    "displayName": "TellMeJoke",
    "description": "It will tell you a random joke",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.getJoke"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.getJoke",
                "title": "Tell Me A Joke"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/request": "^2.48.1"
    }
}
